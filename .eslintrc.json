{
   "extends": [
       "eslint:recommended",
       "plugin:import/errors",
       "plugin:import/recommended",
       "plugin:jsx-a11y/recommended",
       "prettier"

   ],
   "plugins": ["react","import","jsx-a11y"],
   "parserOptions": {
       "ecmaVersion": 2021,
       "sourceType":  "module",
       "ecmaFeatures": {
           "jsx": true
       }
   },
   "env": {
       "es6":true,
       "browser": true,
       "node": true
   }
}